<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto" android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="wrap_content">

    <RelativeLayout
            android:id="@+id/lin_status"
            android:layout_marginBottom="10dp"
            android:padding="5dp"
            android:layout_margin="10dp"
            android:background="@drawable/not_approved_bg"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:elevation="5dp"
            android:layout_height="wrap_content">


        <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
            <TableRow>
                <TextView
                        android:layout_column="1"
                        android:textSize="20dp"
                        android:text=" Rqube ID -"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                <TextView

                        android:textSize="20dp"
                        android:id="@+id/txtRqube"
                        android:text=""
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"/>
            </TableRow>

            <TableRow>
                <TextView
                        android:layout_column="1"
                        android:textSize="20dp"
                        android:textStyle="bold"
                        android:textColor="@color/black"

                        android:text=" Project Name- "
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                <TextView
                        android:textSize="20dp"
                        android:id="@+id/txtProject"

                        android:text=""
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"/>
            </TableRow>


            <TableRow>
                <TextView
                        android:layout_column="1"
                        android:textSize="20dp"
                        android:text=" Building Name - "
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                       />

                <TextView
                        android:textSize="20dp"
                        android:id="@+id/txtBuild"
                        android:text=""
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                       />
            </TableRow>


            <TableRow>
                <TextView
                        android:layout_column="1"

                        android:textSize="20dp"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:text=" Flat No - "
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                <TextView

                        android:textSize="20dp"
                        android:id="@+id/txtFlat"
                        android:text=""
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"/>
            </TableRow>

        </TableLayout>













        <LinearLayout
                android:visibility="gone"
                android:id="@+id/linBackApp"
                android:background="@drawable/round"
                android:layout_marginTop="30dp"
                android:layout_marginRight="15dp"
                android:layout_alignParentRight="true"
                android:layout_width="30dp"
                android:layout_height="wrap_content">
    <ImageView
            android:src="@drawable/ic_done_black_24dp"
            android:layout_width="30dp"
            android:layout_height="30dp"/>

        </LinearLayout>







    </RelativeLayout>




</LinearLayout>